{
  "root": true,
  "env": {
    "es6": true,
    "node": true,
    "mocha": true
  },
  "globals": {
    // "SOME_FOO_VAR": "readonly"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [
          ".js"
        ]
      }
    }
  },
  "extends": [
    "eslint:recommended",
    "plugin:import/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "import"
  ],
  "rules": {

  }


}
